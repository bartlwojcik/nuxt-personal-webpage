{
  "editor.tabSize": 2,
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    }
  ],
  "eslint.format.enable": true,
  "editor.formatOnSave": false,
  "vetur.validation.template": false,
  "vetur.validation.style": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  }
}
